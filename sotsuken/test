<script>
	$(document).ready(function() {
		
		
		$('#selectLocale').parent().addClass('u-w100per');
		
		if ("" !== "true") {
			$('#account').focus();
			$('#btnLocale').hide();
			var $element = $('.input-error').find();
		}
		
		document.querySelector('#password').onkeypress = function(e) {
			if (e.keyCode == 13) {
				document.querySelector('#btnWebLogin').click();
			}
		}
		
		<!-- お知らせ内のファイルの背景色を変更 -->
		var objlist = $(document).find('.dz-preview');
	      for(let item of objlist) {
	    	  item.classList.remove("dz-preview");
              item.classList.add("dz-preview-login");
	      }
	});

	$(function($) {
	    $('#selectLocale').change(function() {
	    	$('#btnLocale').trigger('click');
	    });
	    
	    $('#i-eyecon').on("click", function() {
	    	let eyecss = $('#i-eyecon').attr("class");
	    	if (eyecss === "eyecon") {
	    		$('#i-eyecon').attr("src", "./img/ico_eye.png");
	    		$('#i-eyecon').attr("class", "eyecon on");
	    		$('#password').attr("type", "text");
	    	} else {
	    		$('#i-eyecon').attr("src", "./img/ico_eye-off.png");
	    		$('#i-eyecon').attr("class", "eyecon");
	    		$('#password').attr("type", "password");
	    	}
	    });
	});

	function loadCsrf($button) {

		$.ajax({
            url: '/lcu-web/csrf',
            type: 'GET'
        })
        .done(function(token) {
        	$('input[name="_csrf"]').val(token);
            submitFormButton($button, '/lcu-web/webLogin', '', '');
        });

	}
</script>
