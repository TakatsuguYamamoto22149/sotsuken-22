<script language="javascript">

    _isExcelSubmit = false;

	$(function() {
		$(document).on("keypress", "input:not(.allow_submit)", function(event) {
			return event.which !== 13;
		});
		
		$(document).on('click','.ui-dialog-titlebar-close',function(){
			isSubmit = false;
		});
		
		
	    $('.dataist_js').on('click', function () {
	    	
	    	let options = $(this).attr('list_data').split(',');
	    	
	    	$(this).autocomplete({
	    			source: options,
	    			minLength: 0, 
	    			delay : 1,
	    			autoFocus: false,
	    			scroll:true,
	  	            position:{ my : "right top", at: "right bottom", collision: "flip" }
	    	});
	    	$(this).autocomplete("search", "");
	    });
	});
	
    
    function submitFormButton($button, requestPath, dialogTitle, dialogMessage) {
    
    	var formId = $button.attr('formId');
    	var loading = $button.attr('loading');
    	var dialogType = $button.attr('dialogType');
    	
        var submitStatus = dbClick();
        if (!submitStatus) {
            return false;
        }
        
        saveScrollPosition();
    	
    	if (dialogType == undefined || dialogType == '') {
    		if (loading != 'false') {
    			showLoading();
    		}
    		
    		_isBeforeUnload = false;
    		
    		$('#' + formId).attr('action', requestPath);
    		$('#' + formId).submit();
    
    	} else if (dialogType == 'confirm') {
    		openConfirmDialog(dialogTitle, dialogMessage, function() {
    			if (loading) {
    				showLoading();
    			}
    	        _isBeforeUnload = false;
    			$('#' + formId).attr('action', requestPath);
    			$('#' + formId).submit();
    		}, function() {
    			isSubmit = false;
    		});
    
    	} else if (dialogType == 'message') {
    		openMessageDialog(dialogTitle, dialogMessage, function() {
    			if (loading != 'false') {
    				showLoading();
    			}
    	        _isBeforeUnload = false;
    			$('#' + formId).attr('action', requestPath);
    			$('#' + formId).submit();
    		});
    	}
    }
    
    
    function submitFormLink(formId, requestPath, keyValue) {

        var submitStatus = dbClick();
        if (!submitStatus) {
            return false;
        }
    	
        saveScrollPosition();

    	showLoading();
    	
    	$('#' + formId).attr('action', requestPath);
    	if (keyValue) {
    		$('#' + formId + '_selectedItem').val(keyValue);
    	}
    	$('#' + formId).submit();
    }
    
    
    function submitFormRadio($button, requestPath) {
    
    	var formId = $button.attr('formId');
    	var loading = $button.attr('loading');

        var submitStatus = dbClick();
        if (!submitStatus) {
            return false;
        }

    	if (loading != 'false') {
    		showLoading();
    	}
    	
    	_isBeforeUnload = false;
    	saveScrollPosition();
    	
    	$('#' + formId).attr('action', requestPath);
    	$('#' + formId).submit();
    }
    
    
    function submitFormSelect($button, requestPath) {
    
    	var formId = $button.attr('formId');
    	var loading = $button.attr('loading');
    	
        var submitStatus = dbClick();
        if (!submitStatus) {
            return false;
        }

    	if (loading != 'false') {
    		showLoading();
    	}
    	
    	_isBeforeUnload = false;
    	saveScrollPosition();
    	
    	$('#' + formId).attr('action', requestPath);
    	$('#' + formId).submit();
    }
    
    
     function submitFormButtonForReport($button, requestPath) {
         
         var formId = $button.attr('formId');
         var reportOutputConfirm = $button.attr('reportOutputConfirm');
         
         let existsWindowCloseAlertMsgOffFunction = typeof windowCloseAlertMsgOff == 'function';
        
         if (arguments.length === 0) {
             if (_isExcelSubmit) {
                openConfirmDialog('',
                                  '',
                                  function() {
                                	  _isExcelSubmit = true;
                                	  if (existsWindowCloseAlertMsgOffFunction) {
                                          windowCloseAlertMsgOff();
                                	  }
                                      $('#' + formId).attr('action', requestPath);
                                      $('#' + formId).submit();
                                  },
                                  function() { return false; });
                
             } else {
                 _isExcelSubmit = true;
                 if (existsWindowCloseAlertMsgOffFunction) {
                     windowCloseAlertMsgOff();
                 }
                 $('#' + formId).attr('action', requestPath);
                 $('#' + formId).submit();
             }
         } else {
             var clickKey = arguments[1];
             if (!!isExcelSubmitMap[clickKey] && (reportOutputConfirm != 'false')) {
                 openConfirmDialog('',
                                   '',
                                   function() {
                                	   if (existsWindowCloseAlertMsgOffFunction) {
                                	       windowCloseAlertMsgOff();
                                	   }
                                	   $('#' + formId).attr('action', requestPath);
                                       $('#' + formId).submit();
                                   },
                                   function() {});
             } else {
                 isExcelSubmitMap[clickKey] = true;
                 if (existsWindowCloseAlertMsgOffFunction) {
                     windowCloseAlertMsgOff();
                 }
                 $('#' + formId).attr('action', requestPath);
                 $('#' + formId).submit();
             }
         }
        
     }

     
     function submitFormButtonForReportConfirm($button, requestPath, dialogMessage) {

    	 var dispDialogMessage = dialogMessage.replace(/<br>/g,'\n');
    	 var result = confirm(dispDialogMessage);
    	 if( result ) {
    		 submitFormButtonForReport($button, requestPath);
    	 } 
     }

    
    function openPopUp(requestPath, windowName) {

    	var submitStatus = dbClick();
        if (!submitStatus) {
            return false;
        }
    	
    	var ret = window
    			.open(
    					"common/blank.html",
    					windowName,
    					"width=1100,height=768,status=yes,scrollbars=yes,toolbar=no,location=yes,menubar=no,resizable=yes");
    	ret.focus();
    
    	$('#PopupForm').attr('action', requestPath);
    	$('#PopupForm').attr('target', windowName);
    
    	$('#PopupForm').submit();
    }
    
    
    function showSuccessToast(message) {
    
    	if (!message) {
    		return;
    	}
    	
    	
    	delScrollPosition();
    	
    	toastr.options = {
    		"closeButton" : true,
    		"debug" : false,
    		"newestOnTop" : false,
    		"progressBar" : true,
    		"positionClass" : "toast-top-center",
    		"preventDuplicates" : false,
    		"onclick" : null,
    		"showDuration" : "300",
    		"hideDuration" : "1000",
    		"timeOut" : "5000",
    		"extendedTimeOut" : "1000",
    		"showEasing" : "swing",
    		"hideEasing" : "linear",
    		"showMethod" : "fadeIn",
    		"hideMethod" : "fadeOut"
    	};
    
    	toastr['success'](message);
    }

    
    function _showErrorToast(message) {
    
    	if (!message) {
    		return;
    	}
    	
    	
    	delScrollPosition();
    	
    	toastr.options = {
    		"closeButton" : true,
    		"debug" : false,
    		"newestOnTop" : false,
    		"progressBar" : true,
    		"positionClass" : "toast-top-center",
    		"preventDuplicates" : false,
    		"onclick" : null,
    		"showDuration" : "300",
    		"hideDuration" : "1000",
    		"timeOut" : "0",
    		"extendedTimeOut" : "0",
    		"showEasing" : "swing",
    		"hideEasing" : "linear",
    		"showMethod" : "fadeIn",
    		"hideMethod" : "fadeOut"
    	};

    	toastr['error'](message);
    }
    
    
    function showLoading() {
    
    	var div = $('<div id="loadingDialog" />').css("position", "fixed").css("top", "0").css("left",
    			"0").css("right", "0").css("bottom", "0").css("background",
    			"rgba(0, 0, 0, 0.1)").css('z-index', '10000');
    
    	var msg = "" + "<div class='c-loading'>"
    			+ "	<div class='c-loading-container'>"
    			+ "		<div class='c-loading-icon-container'>"
    			+ "			<div class='c-loading-icon'>" + "			</div>" + "		</div>"
    			+ "		<div class='c-loading-text'>" + "			<p>LOADING</p>"
    			+ "		</div>" + "	</div>" + "</div>";
    
    	div.append(msg);
    
    	$('body').append(div);
    }

    function closeLoading() {
        
    	$('#loadingDialog').remove();

    }

    
    
    
    function exPostPopup(formidx, displayName, actionPath, openParam) {
    	postPopup(formidx, displayName, actionPath, openParam);
    }
    
    
    
    var popupWinList = new Array();
    var popupOpening = false;
    function postPopup(formidx, displayName, actionPath, openParam) {
    	try {
    		if (!popupOpening) {
    			var targetWin = popupWinList[displayName];
    			if (!targetWin || targetWin.closed) {
    				popupOpening = true;
    				targetWin = window.open('common/blank.html', displayName, openParam);
    				popupWinList[displayName] = targetWin;
    				popupOpening = false;
    
    				document.forms[formidx].target = displayName;
    				document.forms[formidx].method = 'post';
    				document.forms[formidx].action = actionPath;
    				_isBeforeUnload = false;
    				document.forms[formidx].submit();
    				_isBeforeUnload = true;
    			} else {
    				targetWin.focus();
    			}
    		}
    	} catch (e) {
    		popupWinList = new Array();
    		popupOpening = false;
    	}
    }
    
    function coopePostPopup(formidx, displayName, actionPath, openParam) {
        try {
            if (!popupOpening) {
                var targetWin = popupWinList[displayName];
                if (!targetWin || targetWin.closed) {
                    popupOpening = true;
                    targetWin = window.open('common/blank.html', displayName, openParam);
                    popupWinList[displayName] = targetWin;
                    popupOpening = false;
                    document.forms[formidx].target = displayName;
                    document.forms[formidx].method = 'post';
                    document.forms[formidx].action = actionPath;
                    _isBeforeUnload = false;
                    document.forms[formidx].submit();
                    _isBeforeUnload = true;
                }
                else {
                	if ('' == '1') {
                	    var dialogTitle = "";
                	    var dialogMessage = "";
                	    openConfirmDialog(dialogTitle, dialogMessage, function() {
                            document.forms[formidx].target = displayName;
                            document.forms[formidx].method = 'post';
                            document.forms[formidx].action = actionPath;
                            _isBeforeUnload = false;
                            document.forms[formidx].submit();
                            _isBeforeUnload = true;
                        });
                	}
                	else {
                		document.forms[formidx].target = displayName;
                        document.forms[formidx].method = 'post';
                        document.forms[formidx].action = actionPath;
                        _isBeforeUnload = false;
                        document.forms[formidx].submit();
                        _isBeforeUnload = true;
                	}
                }
            }
        } catch (e) {
            popupWinList = new Array();
            popupOpening = false;
        }
    }
    
    
    
    
    function exPostSubmit(formidx, actionPath) {
    	return postSubmit(formidx, actionPath);
    }
    
    
    function setComboBox($select, itemList) {
    	 $select.children(":gt(0)").remove();
    	 for(let item of itemList) {
    		 $select.append($("<option>").val(item.value).text(item.label))
    	 }
    }
    
    function openWin(url, windowName, preloginPath) {

        if (preloginPath) {
            jQuery.ajax({
                type: 'POST',
                url: preloginPath,
                data: '',
                async: false,
                success: function (nodata) {},
                dataType: 'text'
            });
        }

        coopePostPopup('home_systemCooperationLink',
                windowName,
                url,
                'location=no, directories=no, resizable=yes, status=no, toolbar=no, menubar=no, width=1000, height=650,left=0, top=0, scrollbars=yes');
    }
    
  
  function saveScrollPosition() {
      sessionStorage.setItem('' + '_scrollPosition', $(window).scrollTop());
  }

  
  function delScrollPosition() {
	  var positionlength = sessionStorage.length;
      var n = 0;
      var scrollCnt = 0;
      const regex = RegExp('^(?=.*)(?=.*scrollPosition).*$');
      while (n < (positionlength - scrollCnt)) {
    	  var key = sessionStorage.key(n);
    	  if (regex.test(sessionStorage.key(n))) {
    		  sessionStorage.removeItem(key);
    		  scrollCnt++;
    	  }
    	  else {
    		  n++;
    	  }
      }
  }
  
  
  function ClearStorage() {
	  sessionStorage.clear();
  }

</script>

<script language="javascript">
	
	
	isSubmit = false;
	isFileSubmit = false;
	isCsvSubmit = false;
	isExcelSubmit = false;
	isPdfSubmit = false;
	isChildOpen = false;
	isCsvSubmitMap = new Array();
	isExcelSubmitMap = new Array();
	
	function dbClick() {
	    
	    if (isSubmit) {
	        return false;
	    }
	    isSubmit = true;
	    return true;
	}
	
	function dbLinkClick(arg) {
	    if (!isSubmit) {
	        isSubmit = true;
	        document.location.href = arg;
	    }
	}
	
	function dbSubmitClick(arg, form) {
	  if (!isSubmit) {
	    isSubmit = true;
	    form.action = arg;
	    form.submit();
	  }
	}
	
	function dbFileClick() {
	    
	    if (isFileSubmit) {
	        
	        if (confirm('<lc:message key="sc.0000.i2020"/>')) {
	            return true;
	        } else {
	            return false;
	        }
	    } else {
	        isFileSubmit = true;
	        return true;
	    }
	}
	
	function dbCsvClick() {
	    if (arguments.length === 0) {
	        
	        if (isCsvSubmit) {
	            
	            if (confirm('<lc:message key="sc.0000.i2021"/>')) {
	                return true;
	            } else {
	                return false;
	            }
	        } else {
	            isCsvSubmit = true;
	            return true;
	        }
	    } else {
	        
	        var clickKey = arguments[0];
	        
	        if (!!isCsvSubmitMap[clickKey]) {
	            
	            if (confirm('<lc:message key="sc.0000.i2021"/>')) {
	                return true;
	            } else {
	                return false;
	            }
	        } else {
	            isCsvSubmitMap[clickKey] = true;
	            return true;
	        }
	    }
	}
	
	function dbExcelClick() {
	    if (arguments.length === 0) {
	        
	        if (isExcelSubmit) {
	            
	            if (confirm('<lc:message key="sc.0000.i2022"/>')) {
	                return true;
	            } else {
	                return false;
	            }
	        } else {
	            isExcelSubmit = true;
	            return true;
	        }
	    } else {
	        
	        var clickKey = arguments[0];
	        
	        if (!!isExcelSubmitMap[clickKey]) {
	            
	            if (confirm('<lc:message key="sc.0000.i2022"/>')) {
	                return true;
	            } else {
	                return false;
	            }
	        } else {
	            isExcelSubmitMap[clickKey] = true;
	            return true;
	        }
	    }
	}
	
	function dbPrintClick() {
	    
	    if (isPdfSubmit) {
	        alert("現在印刷処理中です。しばらくお待ちください。");
	        return false;
	    } else {
	        isPdfSubmit = true;
	        return true;
	    }
	}
	
	function dbChildOpen(url, width, height) {
	    if (!isChildOpen) {
	        isChildOpen = true;
	        childWindow = window.open(url, 'subWindow', 'width=' + width + ',height=' + height + ', scrollbars=yes, resizable=yes, status=yes');
	        childWindow.focus();
	    } else {
	        
	        if(navigator.userAgent.indexOf('Safari/85') != -1){
	            
	            childWindow = window.open(url, 'subWindow', 'width=' + width + ',height=' + height + ', scrollbars=yes, resizable=yes');
	            childWindow.focus();
	        
	        } else {
	            if (childWindow.closed) {
	                childWindow = window.open(url, 'subWindow', 'width=' + width + ',height=' + height + ', scrollbars=yes, resizable=yes');
	                childWindow.focus();
	            } else {
	                alert("子ウィンドウが既に開いています。");
	            }
	        }
	    }
	}
	
	function dbDemoChildOpen(url) {
	    var width = window.innerWidth;
	    var height = window.innerHeight;
	    var topLeft = "";
	
	    if (typeof width == 'undefined' || typeof height == 'undefined') {
	        
	        if (document.getElementById && (document.compatMode=='CSS1Compat')) {
	            width = document.documentElement.clientWidth;
	            height = document.documentElement.clientHeight;
	            var top = document.documentElement.scrollTop;
	            var left = document.documentElement.scrollLeft;
	            topLeft = ",top=" + top + ",left=" + left;
	
	        
	        } else if (document.all) {
	            width = document.body.clientWidth;
	            height = document.body.clientHeight;
	            var top = window.screenTop;
	            var left = window.screenLeft;
	            topLeft = ",top=" + top + ",left=" + left;
	        }
	    }
	
	    var option = "width=" + width + ",height=" + height + topLeft
	        + ",menubar=yes,toolbar=yes,location=yes,status=yes,resizable=yes,scrollbars=yes";
	
	    if (!isChildOpen) {
	        isChildOpen = true;
	        childWindow = window.open(url, 'subWindow', option);
	        childWindow.focus();
	
	    } else {
	        
	        if (navigator.userAgent.indexOf('Safari/85') != -1) {
	            
	            childWindow = window.open(url, 'subWindow', option);
	            childWindow.focus();
	        
	        } else {
	            if (childWindow.closed) {
	                childWindow = window.open(url, 'subWindow', option);
	                childWindow.focus();
	            } else {
	                alert("子ウィンドウが既に開いています。");
	            }
	        }
	    }
	}

</script>


